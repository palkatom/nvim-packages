{
    "pack/setup/opt/setup-*/plugin/setup.vim": {
        "type": "setup",
        "template": [
            "call setup#init_plugins(feature#{}#plugins)"
        ]
    },
    "pack/features/start/*/plugin/config.vim": {
        "type": "feature",
        "alternate": "pack/features/start/{}/autoload/feature/{}.vim",
        "template": [
            "call feature#helpers#load_plugins(feature#{}#plugins)"
        ]
    },
    "pack/features/opt/*/plugin/config.vim": {
        "type": "ofeature",
        "alternate": "pack/features/opt/{}/autoload/feature#{}.vim",
        "template": [
            "silent packadd setup-{}",
            "call feature#helpers#load_plugins(feature#{}#plugins)"
        ]
    },
    "pack/config/opt/config-*/plugin/setup.vim": {
        "type": "config",
        "template": [
            "if exists(\"g:config_{}_loaded\")",
            "  finish",
            "endif",
            "let g:config_{}_loaded = 1"
        ]
    },
    "**/autoload/feature/*.vim": {
        "template": [
            "let feature#{basename}#plugins = []"
        ]
    }
}
